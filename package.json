{
  "name": "rakuishi.nuxtjs",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "rakuishi <rakuishi@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "processmd":
      "processmd content/archives/*.md --stdout --outputDir static/content/archives > static/content/archives/index.json && processmd content/*.md --stdout --outputDir static/content > static/content/index.json"
  },
  "dependencies": {
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "processmd": "^4.2.1"
  }
}
